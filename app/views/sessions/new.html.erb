<h1>Login</h1>
   <%= form_tag('/signin', method: "post") do %>
   <%= label_tag :name %>
   <%= collection_select(:user, :name, User.all, :name, :name, prompt: 'Please select your username from this dropdown') %><br>
   <%= label_tag :password%>
   <%= password_field_tag :password%><br>
   <%= submit_tag "Sign In"%>
<%end%>